{
    "name": "@parsify/hello",
    "version": "0.0.8",
    "description": "Parsify plugin for showing some text",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "author": "Antoni Kępiński <antoni@kepinski.ch> (https://www.kepinski.ch)",
    "bugs": {
        "url": "https://github.com/parsify-dev/plugin-boilerplate-typescript/issues"
    },
    "scripts": {
        "prebuild": "del-cli dist",
        "esm": "tsc --module esnext && cpy dist/index.js dist --rename index.esm.js",
        "cjs": "tsc --module commonjs",
        "build": "npm run esm && npm run cjs",
        "test": "xo && c8 ava",
        "prepublishOnly": "npm run build"
    },
    "engines": {
        "node": ">=10"
    },
    "license": "MIT",
    "repository": "parsify-dev/plugin-boilerplate-typescript",
    "homepage": "https://github.com/parsify-dev/plugin-boilerplate-typescript",
    "keywords": [
        "parsify",
        "parsify-plugin",
        "plugin",
        "expression-parser",
        "parser",
        "expression"
    ],
    "devDependencies": {
        "@sindresorhus/tsconfig": "^2.0.0",
        "@types/node": "^16.11.6",
        "ava": "^3.15.0",
        "c8": "^7.10.0",
        "coveralls": "^3.1.1",
        "cpy-cli": "^3.1.1",
        "del-cli": "^4.0.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    },
    "sideEffects": false,
    "ava": {
        "extensions": {
            "ts": "module"
        },
        "nonSemVerExperiments": {
            "configurableModuleFormat": true
        },
        "nodeArguments": [
            "--loader=ts-node/esm"
        ]
    },
    "xo": {
        "ignores": [
            "test.ts"
        ],
        "rules": {
            "node/prefer-global/process": "off"
        }
    },
    "dependencies": {
        "@parsify/core": "^1.0.0"
    }
}
